function copiaInfoB314P344() {
  const ssO = SpreadsheetApp.openById("1YfS4g2qHrcVSlTK-X_-LD183hzZ74dKmRIbC10dHpZE");
  const range = ssO.getSheetByName("AC2.0").getRange("B314:P344");
  const sheetD = SpreadsheetApp.openById("1quYFZl30fhntghAo7hSkNC_xEddo--n6TZIw8zj15Sg").getSheetByName("REPORTEO");
  var values = range.getValues();
  sheetD.getRange(1,1,31,15).setValues(values);
  // range.copyTo(sheetD.getRange("A1"),{ contentsOnly: false });
}


// VERSION CORREGIDA:
function copiaInfoB314P344() {
  const ssO = SpreadsheetApp.openById("1YfS4g2qHrcVSlTK-X_-LD183hzZ74dKmRIbC10dHpZE");
  const rangoOrigen = ssO.getSheetByName("AC2.0").getRange("B314:P344");
  const sheetD = SpreadsheetApp.openById("1quYFZl30fhntghAo7hSkNC_xEddo--n6TZIw8zj15Sg").getSheetByName("REPORTEO");
  var values = rangoOrigen.getValues();
  var ultimaFila = sheetD.getLastRow();
  var columnaInicio = 1;
  const filas = values.length;
  const columnas = values[0].length;
  const filaDestino = sheetD.getLastRow() + 1;

  const rangoDestino = sheetD.getRange(filaDestino, 1, filas, columnas);
    const coloresNegros = Array.from({ length: filas }, () =>
    Array.from({ length: columnas }, () => "black")
  );
  rangoDestino.setBackgrounds(rangoOrigen.getBackgrounds());
  rangoDestino.setFontFamilies(rangoOrigen.getFontFamilies());
  rangoDestino.setFontSizes(rangoOrigen.getFontSizes());
  rangoDestino.setFontWeights(rangoOrigen.getFontWeights());
  rangoDestino.setHorizontalAlignments(rangoOrigen.getHorizontalAlignments());
  rangoDestino.setVerticalAlignments(rangoOrigen.getVerticalAlignments());
  rangoDestino.setWraps(rangoOrigen.getWraps());
  // rangoDestino.setNumberFormats(rangoOrigen.getNumberFormats());
  sheetD.getRange(ultimaFila + 1, columnaInicio, values.length, values[0].length).setValues(values);
  rangoDestino.setFontColors(coloresNegros);
}
